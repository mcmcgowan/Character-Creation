{
  "name": "character-creation",
  "version": "1.0.0",
  "description": "React app to create DnD characters using SRD api",
  "main": "src/server/server.js",
  "scripts": {
    "start": "nodemon src/server/index.js",
    "build": "webpack -w",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --open --hot --progress --color \" \"nodemon ./server/server.js\""
  },
  "nodemonConfig": {
    "ignore": [
      "build",
      "client",
      "__tests__"
    ]
  },
  "author": "Matthew McGowan",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.27.2",
    "concurrently": "^7.4.0",
    "express": "^4.18.1",
    "mongoose": "^6.6.0",
    "node-fetch": "^3.2.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.4.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.19",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0",
    "webpack-hot-middleware": "^2.25.2"
  }
